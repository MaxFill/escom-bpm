<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog 
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd"> 
    
    <changeSet id="folders_add_link_to_partner" author="filatov" context="updatedata" runOnChange="true">  
        <addColumn tableName="folders">
            <column name="CompanyDefault" type="INT" >
                <constraints nullable="true"/>
            </column>
            <column name="IsInheritCompany" type="BOOLEAN" >
                <constraints nullable="true"/>
            </column>
            <column name="IsInheritDocType" type="BOOLEAN" >
                <constraints nullable="true"/>
            </column>
            <column name="IsInheritPartner" type="BOOLEAN" >
                <constraints nullable="true"/>
            </column>
        </addColumn>
    </changeSet>

</databaseChangeLog>